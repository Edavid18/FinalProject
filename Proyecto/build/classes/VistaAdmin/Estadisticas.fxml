<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="695.0" prefWidth="1028.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VistaAdmin.EstadisticasController">
    <stylesheets>
        <URL value="@estadisticas.css" />
    </stylesheets>
    <children>
        <Pane id="bar" prefHeight="49.0" prefWidth="1028.0" stylesheets="@estadisticas.css">
            <children>
                <ImageView fitHeight="36.0" fitWidth="28.0" layoutX="23.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/LOGO%202.png" />
                    </image>
                </ImageView>
            </children>
        </Pane>
        <LineChart fx:id="linea" layoutX="20.0" layoutY="70.0">
            <xAxis>
                <CategoryAxis fx:id="xAxis" label="Months" tickLabelRotation="0"  />
                
            </xAxis>
            <yAxis>
                <NumberAxis label="" />
            </yAxis>
        </LineChart>
    </children>
</AnchorPane>
